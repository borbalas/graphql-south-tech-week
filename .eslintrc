{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true,
    "superInFunctions": false,
    "classes": true,
    "modules": [2]
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "complexity": [0, 11],
    "no-cond-assign": [2],
    "no-console": [1],
    "no-constant-condition": [2],
    "no-debugger":[1],
    "no-dupe-args": [2],
    "no-dupe-keys": [2],
    "no-duplicate-case": [2],
    "no-empty": [2],
    "no-ex-assign": [2],
    "no-extra-boolean-cast": [2],
    "no-extra-parens": [0],
    "no-extra-semi": [2],
    "no-func-assign": [2],
    "no-inner-declarations": [0, "both"],
    "no-irregular-whitespace": [2],
    "no-negated-in-lhs": [2],
    "no-obj-calls": [2],
    "no-sparse-arrays": [2],
    "no-unreachable": [2],
    "use-isnan": [2],
    "valid-typeof": [2],
    "no-unexpected-multiline": [2],
    "consistent-return": [2],
    "curly": [2, "multi-line"],
    "default-case": [2],
    "dot-notation": [2, { "allowKeywords": true }],
    "eqeqeq": [2],
    "guard-for-in": [0],
    "no-alert": [2],
    "no-caller": [2],
    "no-else-return": [0],
    "no-empty-label": [2],
    "no-eq-null": [0],
    "no-eval": [2],
    "no-extend-native": [1],
    "no-extra-bind": [2],
    "no-fallthrough": [2],
    "no-floating-decimal": [2],
    "no-implied-eval": [2],
    "no-invalid-this": [2],
    "no-iterator": [2],
    "no-labels": [2],
    "no-lone-blocks": [2],
    "no-loop-func": [2],
    "no-multi-spaces": [2],
    "no-multi-str": [2],
    "no-native-reassign": [2],
    "no-new-func": [2],
    "no-new-wrappers": [2],
    "no-new": [2],
    "no-octal": [2],
    "no-octal-escape": [2],
    "no-param-reassign": [2],
    "no-proto": [2],
    "no-redeclare": [2],
    "no-return-assign": [2],
    "no-script-url": [2],
    "no-self-compare": [2],
    "no-sequences": [2],
    "no-throw-literal": [2],
    "no-unused-expressions": [2],
    "no-useless-call": [2],
    "no-useless-concat": [2],
    "no-void": [0],
    "no-warning-comments": [0, { "terms": ["todo", "fixme", "xxx"], "location": "start" }],
    "no-with": [2],
    "radix": [0],
    "vars-on-top": [0],
    "wrap-iife": [2],
    "yoda": [2, "never", { "exceptRange": true }],
    "init-declarations": [2, "always"],
    "no-delete-var": [2],
    "no-catch-shadow": [2],
    "no-shadow-restricted-names": [2],
    "no-label-var": [2],
    "no-shadow": [2],
    "no-undef-init": [2],
    "no-undef": [2],
    "no-undefined": [2],
    "no-unused-vars": [1, { "vars": "all", "args": "after-used" }],
    "no-use-before-define": [2],
    "array-bracket-spacing": [1, "always"],
    "block-spacing": [1, "always"],
    "brace-style": [1, "1tbs", { "allowSingleLine": true }],
    "camelcase": [0],
    "comma-spacing": [1],
    "consistent-this": [0, "that"],
    "eol-last": [1],
    "func-names": [0],
    "func-style": [0, "declaration"],
    "indent": [1, 2],
    "jsx-quotes": [1, "prefer-single"],
    "key-spacing": [2, { "beforeColon": false, "afterColon": true }],
    "max-nested-callbacks": [0, 2],
    "new-cap": [0],
    "new-parens": [2],
    "no-array-constructor": [2],
    "no-continue": [2],
    "no-inline-comments": [1],
    "no-mixed-spaces-and-tabs": [2, false],
    "no-lonely-if": [2],
    "no-new-object": [2],
    "no-trailing-spaces": [2, {"skipBlankLines": true}],
    "no-underscore-dangle": [2],
    "no-unneeded-ternary": [2],
    "object-curly-spacing": [2, "always"],
    "one-var": [0],
    "operator-assignment": [0, "always"],
    "operator-linebreak": [2, "after"],
    "padded-blocks": [0],
    "quoted-props": [0],
    "quotes": [1, "single"],
    "semi-spacing": [2, { "before": false, "after": true }],
    "semi": [2],
    "sort-vars": [0],
    "space-after-keywords": [2, "always"],
    "space-before-function-paren": [2, { "anonymous": "always", "named": "always" }],
    "space-before-blocks": [1, "always"],
    "space-in-parens": [2, "never"],
    "space-infix-ops": [2],
    "space-return-throw-case": [2],
    "space-unary-ops": [2, { "words": true, "nonwords": false }],
    "wrap-regex": [2],
    "arrow-parens": [2,"always"],
    "arrow-spacing": [2, {"before": true, "after": true}],
    "generator-star-spacing": [2, "both"],
    "no-const-assign": [2],
    "constructor-super": [2],
    "no-class-assign": [2],
    "no-dupe-class-members": [2],
    "no-this-before-super": [2],
    "no-var": [2],
    "object-shorthand": [2, "always"],
    "prefer-arrow-callback": [2],
    "prefer-const": [0],
    "prefer-spread": [2],
    "prefer-template": [2],
    "require-yield": [2]
  }
}
